<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script><script>
$(function () {
	$("#accordion").accordion({
		heightStyle: "content",
		collapsible: true,
/* https://stackoverflow.com/a/26377773/5447232
   минус высота верхнего стикера - - $(".toolbar-top.stick").height()
*/
//		active: false,
		activate: function( event, ui ) {
			if(!$.isEmptyObject(ui.newHeader.offset())) {
				$('html:not(:animated), body:not(:animated)').animate({ scrollTop: ui.newHeader.offset().top - $(".toolbar-top.stick").height() }, 'slow');
       		}
    	}
/* /https://stackoverflow.com/a/26377773/5447232*/
	});
});
</script>
